title = {{Free Tools and Strategies for the Generation of 3D Finite Element Meshes: Modeling of the Cardiac Structures}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23762031 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3670575 http://www.hindawi.com/journals/ijbi/2013/540571/},
