@article{Prausnitz1998,
abstract = {The objective of this study was to collect a comprehensive database of ocular tissue permeability measurements found in a review of the literature to guide models for drug transport in the eye. Well over 300 permeability measurements of cornea, sclera, and conjunctiva, as well as corneal epithelium, stroma, and endothelium, were obtained for almost 150 different compounds from more than 40 different studies. In agreement with previous work, the corneal epithelium was shown generally to control transcorneal transport, where corneal stroma and endothelium contribute significantly only to the barrier for small, lipophilic compounds. In addition, other quantitative comparisons between ocular tissues are presented. This study provides an extensive database of ocular tissue permeabilities, which should be useful for future development and validation of models to predict rates of drug delivery to the eye.},
author = {Prausnitz, Mark R. and Noonan, Jeremy S.},
doi = {10.1021/js9802594},
