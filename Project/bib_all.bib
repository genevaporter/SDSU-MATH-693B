issn = {11782005},
journal = {International Journal of COPD},
keywords = {Bronchodilators,Chronic obstructive pulmonary disease,Exacerbation,Inhaled corticosteroids,Model,Predictor},
month = {jan},
pages = {201--209},
pmid = {25670896},
publisher = {Dove Press},
title = {{A score to predict short-term risk of COPD exacerbations (SCOPEX)}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25670896 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4315304 http://www.dovepress.com/a-score-to-predict-short-term-risk-of-copd-exacerbations-scopex-peer-reviewed-article-COPD},
