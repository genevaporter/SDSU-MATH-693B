issn = {00219991},
journal = {Journal of Computational Physics},
keywords = {Finite elements,Implicit-explicit schemes,Moving grid finite elements,Moving meshes,Reaction-diffusion systems,Schnakenberg model,Time-stepping methods},
month = {may},
