file = {:home/geneva/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Murphy, Madzvamuse - 2019 - A moving grid finite element method applied to a mechanobiochemical model for 3D cell migration.pdf:pdf},
keywords = {Mechanobiochemical model,cell motility,force balance equation,moving boundary problem,moving grid finite elements,partial differential equations,reaction-diffusion equations,viscoelastic},
month = {mar},
title = {{A moving grid finite element method applied to a mechanobiochemical model for 3D cell migration}},
url = {http://arxiv.org/abs/1903.09535},
year = {2019}
}
